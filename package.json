    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@umijs/fabric": "^2.5.6",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "docz": "^2.3.1",
    "gatsby-plugin-less": "^4.7.0",
    "husky": "^5.1.1",
    "less": "^4.1.1",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "prismjs": "^1.23.0",
    "raw-loader": "^4.0.2",
    "react": "^17.0.1",
    "react-copy-to-clipboard": "^5.0.3",
